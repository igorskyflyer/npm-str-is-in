        "find",
        "search",
        "lookup",
        "utilities",
        "helper",
        "typescript",
        "javascript",
        "igor dimitrijević",
        "igorskyflyer"
    ],
    "license": "MIT",
    "author": "Igor Dimitrijević <support@igorskyflyer.me> (https://github.com/igorskyflyer)",
    "funding": "https://ko-fi.com/igorskyflyer",
    "homepage": "https://github.com/igorskyflyer/npm-str-is-in",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/igorskyflyer/npm-str-is-in.git"
    },
    "bugs": {
        "url": "https://github.com/igorskyflyer/npm-str-is-in/issues"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.2.2",
        "@igorskyflyer/biome-config": "^2.1.0",
        "@types/node": "^22.18.0",
        "typescript": "^5.9.2",
